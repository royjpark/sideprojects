import java.util.Scanner;
import java.lang.Math;

public class PI{

     public static void main(String []args){
        System.out.println("Enter how many digits of PI you would like to print out: ");
        Scanner scan = new Scanner(System.in);
        
        int p = scan.nextInt();
        System.out.format("%." + p + "f%n", Math.PI);
        
        scan.close();
     }
}
